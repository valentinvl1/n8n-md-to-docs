{
  "name": "markdown-to-gdocs-function",
  "version": "1.0.0",
  "description": "Firebase Function to convert Markdown to Google Docs",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./lib --target node",
    "start": "bun run build && node lib/index.js",
    "dev": "bun --watch src/index.ts"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "express": "^4.18.2",
    "googleapis": "^128.0.0",
    "marked": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/marked": "^5.0.2",
    "@types/node": "^18.0.0",
    "bun-types": "latest"
  },
  "private": true
}
